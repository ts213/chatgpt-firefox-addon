{
  "manifest_version": 2,
  "name": "ChatGPT API",
  "version": "1.3",
  "browser_action": {
    "default_popup": "p.html"
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "opts.html"
  },
  "icons": {
    "48": "icon.svg"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      }
    }
  }
}
